{
    "extends": "airbnb-base",
    "rules": {
        "camelcase": "off",
        "indent": ["error", 4],
        "import/extensions": [
            "error",
            "always",
            {
              "js": "never",
              "jsx": "never",
              "mjs": "never"
            }
          ]
      },
      "env": {
          "jquery": true,
          "browser": true
      }
}