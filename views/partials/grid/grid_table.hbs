<div id='cell_image_array'>
    
    {{#each image_uri_array}}
    <div class="card d-none" id="image_{{id}}">
        <img class="card-img-top" src="{{image_uri}}" alt="Card image cap">
        <div class="card-body">
            <h5 class="card-title">{{name}}</h5>
            <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's
                content.</p>
            <a href="#" class="btn btn-primary">Go somewhere</a>
        </div>
    </div>
    {{/each}}
   
</div>
<table id='grid-list' class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
    <thead>
        <tr>
            {{#each column_headers as |header|}}
            <th>{{header}}</th>
            {{/each}}
        </tr>
    </thead>
    <tbody>
        {{#each row_value_list as |value_list|}}
        <tr>
            {{#each value_list as |value|}}
            <td class="hover_cell">{{value}}</a></td>
            {{/each}}
        </tr>

        {{/each}}
    </tbody>
</table>