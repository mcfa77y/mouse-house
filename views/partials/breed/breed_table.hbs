<table id='breed-list' class="mdl-data-table" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>id</th>
            <th>#</th>
            <th>genotype</th>
            <th>male</th>
            <th>female</th>
            <th>pairing</th>
            <th>plug</th>
            <th>litter</th>
            <th>ween</th>
            <th>pup check</th>
            <th>male count</th>
            <th>female count</th>
            <th>Notes</th>
        </tr>
    </thead>
    <tbody>
        {{#each breeds}}
        <tr>
            <td>{{id}}</td>
            <td><a href="breed/{{id_alias}}">{{id_alias}}</a></td>
            <td>{{genotype}}</td>
            <td>{{male_mouse}}</td>
            <td>{{female_mouse}}</td>
            <td>{{pairing_date}}</td>
            <td>{{plug_date}}</td>
            <td>{{litter_date}}</td>
            <td>{{ween_date}}</td>
            <td>{{pup_check_date}}</td>
            <td>{{male_count}}</td>
            <td>{{female_count}}</td>
            <td>{{ellipsis note 32}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
