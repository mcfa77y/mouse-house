<table id='mice' class="mdl-data-table" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>#</th>
            <th>Ear tag</th>
            <th>D.O.B</th>
            <th>Genotype</th>
            <th>Sex</th>
            <th>Notes</th>
            <th>Status</th>
            <th>Cage</th>
            <th>Breed</th>
        </tr>
    </thead>
   <tbody>
        {{#each mice}}
        <tr>
            <th>{{id}}</th>
            <th>{{ear_tag}}</th>
            <th>{{dob}}</th>
            <th>{{genotype}}</th>
            <th>{{sex}}</th>
            <th>{{ellipsis notes 32}}</th>
            <th>{{status}}</th>
            <th>{{create_timestamp}}</th>
            <th>{{modify_timestamp}}</th>
        </tr>
        {{/each}}
   </tbody>
</table>
