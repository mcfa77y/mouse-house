<span class="float-right" data-toggle="tooltip" data-placement="top" title="Add {{model_name}}">
    <a href="{{model_name}}/create" class="btn btn-info btn-fab" role="button" ><i class="material-icons">add</i> </a>
</span>
<span class="float-right" data-toggle="tooltip" data-placement="top" title="Delete {{model_name}}">
    <button id="open-delete-{{model_name}}-modal-button"  type="button" class="btn btn-danger btn-fab" data-toggle="modal" data-target="#confirm-delete-modal"  ><i class="material-icons">delete</i></button>
</span>