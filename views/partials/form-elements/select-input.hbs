<div class="form-group label-floating  col-md-6">
    <label for="{{id}}" class="col-md-3 control-label">{{description}}</label>
    <div class="col-md-9">
        <select class="form-control foo-select" id="{{id}}">
            {{#each items}}
            <option value="{{this}}">{{this}}</option>
            {{/each}}
        </select>
    </div>
</div>
