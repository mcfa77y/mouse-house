<div id='cell_image_array' class='collapse show'>
    <div id="image_row" class="card-deck">
    </div>
</div>

<table id='platemap_table' class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
    <thead>
        <tr>
            {{#each column_headers as |header|}}
            <th>{{header}}</th>
            {{/each}}
        </tr>
    </thead>
    <tbody>
        {{#each grid as |row_name_list|}}
        <tr>
            {{#each row_name_list as |name|}}
            <td class="hover_cell unselected_cell" value="{{@../index}}_{{@index}}">{{name}}</td>
            {{/each}}
        </tr>

        {{/each}}
    </tbody>
</table>

