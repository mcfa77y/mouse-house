<table id='project-list' class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>ID</th>
            <th>Project</th>
            <th>Description</th>
            <th>Updated at</th>
            <th>Created at</th>
        </tr>
    </thead>
    <tbody>
        {{#each projects}}
        <tr>
            <td>{{id}}</td>
            <td><a href="project/{{id}}">{{name}}</a></td>
            <td>{{ellipsis note 32}}</td>
            <td>{{updated_at}}</td>
            <td>{{created_at}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>