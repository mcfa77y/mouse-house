<table id='breed-list' class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>id</th>
            <th>#</th>
            <th>male</th>
            <th>age (wks)</th>
            <th>female</th>
            <th>age (wks)</th>
            <th>pairing</th>
            <th>plug</th>
            <th>pup check</th>
            <th>litter</th>
            <th>ween</th>
            <th>male count</th>
            <th>female count</th>
            <th>Notes</th>
        </tr>
    </thead>
    <tbody>
        {{#each breeds}}
        <tr>
            <td>{{id}}</td>
            <td><a href="breed/{{id_alias}}">{{id_alias}}</a></td>
            <td><a href="/mouse/{{male_mouse.id}}">{{male_mouse.id}}</a></td>
            <td>{{male_mouse.age}}</td>
            <td><a href="/mouse/{{female_mouse.id}}">{{female_mouse.id}}</td>
            <td>{{female_mouse.age}}</td>
            <td>{{pairing_date}}</td>
            <td>{{plug_date}}</td>
            <td>{{pup_check_date}}</td>
            <td>{{litter_date}}</td>
            <td>{{ween_date}}</td>
            <td>{{male_count}}</td>
            <td>{{female_count}}</td>
            <td>{{ellipsis note 32}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
